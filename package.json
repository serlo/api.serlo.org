{
  "name": "@serlo/api.serlo.org",
  "version": "0.18.1",
  "private": true,
  "bugs": {
    "url": "https://github.com/serlo/api.serlo.org/issues"
  },
  "repository": "serlo/api.serlo.org",
  "license": "Apache-2.0",
  "author": "Serlo Education e.V.",
  "scripts": {
    "_build": "tsdx build --target node --format cjs --tsconfig tsconfig.prod.json",
    "_eslint": "eslint \"{{__fixtures__,__tests-pacts__,__tests__,scripts,src}/**/*,*}.{js,jsx,ts,tsx}\"",
    "_prettier": "prettier .",
    "build:server": "yarn _build --entry src/server.ts",
    "build:swr-queue-worker": "yarn _build --entry src/swr-queue-worker.ts",
    "build:types": "yarn _build --entry src/types.ts",
    "changelog": "ts-node scripts/changelog",
    "deploy:image": "ts-node scripts/deploy",
    "format": "npm-run-all -c format:*",
    "format:eslint": "yarn _eslint --fix",
    "format:prettier": "yarn _prettier --write",
    "license": "ts-node scripts/license-headers",
    "lint": "npm-run-all --parallel lint:prettier lint:tsc lint:eslint",
    "lint:eslint": "yarn _eslint",
    "lint:prettier": "yarn _prettier --check",
    "lint:tsc": "tsc --noEmit",
    "pacts": "npm-run-all pacts:clean pacts:serlo-org pacts:serlo-org-database-layer",
    "pacts:clean": "rimraf pacts",
    "pacts:publish": "ts-node scripts/publish-pacts",
    "pacts:serlo-org": "jest --config jest.config-pacts-serlo-org.js --runInBand",
    "pacts:serlo-org-database-layer": "jest --config jest.config-pacts-serlo-org-database-layer.js --runInBand",
    "preregenerate-types": "graphql-codegen --config codegen.yml",
    "regenerate-types": "ts-node scripts/regenerate-types-package",
    "start": "npm-run-all start:redis start:server",
    "start:redis": "docker-compose up --detach",
    "start:server": "tsdx watch --entry src/server.ts --target node --format cjs --tsconfig tsconfig.prod.json --onSuccess \"node dist\"",
    "start:swr-queue-worker": "tsdx watch --entry src/swr-queue-worker.ts --target node --format cjs --tsconfig tsconfig.prod.json --onSuccess \"node dist\"",
    "stop:redis": "docker-compose stop",
    "test": "jest --config jest.config.js --runInBand"
  },
  "dependencies": {
    "@sentry/node": "^6.0.0",
    "apollo-datasource-rest": "^0.9.0",
    "apollo-server": "^2.0.0",
    "apollo-server-caching": "^0.5.0",
    "apollo-server-express": "^2.0.0",
    "apollo-server-types": "^0.6.0",
    "basic-auth": "^2.0.0",
    "bee-queue": "^1.0.0",
    "bull-arena": "^3.0.0",
    "dotenv": "^8.0.0",
    "express": "^4.0.0",
    "fp-ts": "^2.0.0",
    "graphql": "^15.0.0",
    "graphql-parse-resolve-info": "^4.0.0",
    "graphql-playground-middleware-express": "^1.0.0",
    "graphql-tag": "^2.0.0",
    "graphql-type-json": "^0.3.0",
    "io-ts": "^2.0.0",
    "io-ts-types": "^0.5.0",
    "jsonwebtoken": "^8.0.0",
    "loglevel": "^1.0.0",
    "monocle-ts": "^2.0.0",
    "msgpack": "^1.0.0",
    "newtype-ts": "^0.3.0",
    "node-fetch": "^2.6.1",
    "ramda": "^0.27.0",
    "redis": "^3.0.0",
    "redlock": "^3.0.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.0.0",
    "@graphql-codegen/typescript": "^1.0.0",
    "@inyono/changelog": "^0.2.0",
    "@inyono/copyright-headers": "^0.1.0",
    "@microsoft/api-extractor": "^7.0.0",
    "@pact-foundation/pact": "^9.0.0",
    "@pact-foundation/pact-node": "^10.0.0",
    "@types/basic-auth": "^1.0.0",
    "@types/bull-arena": "^3.0.0",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.0.0",
    "@types/msgpack": "^0.0.30",
    "@types/node": "^14.0.0",
    "@types/node-fetch": "^2.0.0",
    "@types/ramda": "^0.27.35",
    "@types/redis": "^2.0.0",
    "@types/redlock": "^3.0.0",
    "@types/rimraf": "^3.0.0",
    "@types/semver": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "apollo-server-testing": "^2.0.0",
    "babel-plugin-import-graphql": "^2.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-react": "^7.0.0",
    "glob": "^7.0.0",
    "jest": "^26.0.0",
    "jest-transform-graphql": "^2.0.0",
    "msw": "^0.26.0",
    "npm-run-all": "^4.0.0",
    "prettier": "^2.0.0",
    "prettier-plugin-packagejson": "^2.0.0",
    "rimraf": "^3.0.0",
    "semver": "^7.0.0",
    "ts-jest": "^26.0.0",
    "ts-node": "^9.0.0",
    "tsdx": "^0.14.0",
    "typescript": "^4.0.0"
  },
  "engines": {
    "node": "^14.0.0"
  }
}
