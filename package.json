{
  "name": "@serlo/api.serlo.org",
  "version": "0.9.0",
  "private": true,
  "bugs": {
    "url": "https://github.com/serlo/api.serlo.org/issues"
  },
  "repository": "serlo/api.serlo.org",
  "license": "Apache-2.0",
  "author": "Serlo Education e.V.",
  "scripts": {
    "_eslint": "eslint \"{{__fixtures__,__tests-pacts__,__tests__,scripts,src}/**/*,*}.{js,jsx,ts,tsx}\"",
    "_prettier": "prettier .",
    "build": "tsdx build --target node --format cjs --tsconfig tsconfig.prod.json",
    "changelog": "ts-node scripts/changelog",
    "deploy:image": "ts-node scripts/deploy",
    "format": "npm-run-all -c format:*",
    "format:eslint": "yarn _eslint --fix",
    "format:prettier": "yarn _prettier --write",
    "license": "ts-node scripts/license-headers",
    "lint": "npm-run-all --parallel lint:prettier lint:tsc lint:eslint",
    "lint:eslint": "yarn _eslint",
    "lint:prettier": "yarn _prettier --check",
    "lint:tsc": "tsc --noEmit",
    "pacts": "jest --config jest.config-pacts.js",
    "pacts:publish": "ts-node scripts/publish-pacts",
    "preregenerate-types": "graphql-codegen --config codegen.yml",
    "regenerate-types": "ts-node scripts/regenerate-types-package",
    "start": "tsdx watch  --target node --format cjs --tsconfig tsconfig.prod.json --onSuccess \"node dist\"",
    "test": "jest"
  },
  "dependencies": {
    "@sentry/node": "^5.0.0",
    "apollo-datasource-rest": "^0.9.0",
    "apollo-server": "^2.0.0",
    "apollo-server-caching": "^0.5.0",
    "apollo-server-express": "^2.0.0",
    "apollo-server-types": "^0.5.0",
    "dotenv": "^8.0.0",
    "express": "^4.0.0",
    "fp-ts": "^2.0.0",
    "graphql": "^15.0.0",
    "graphql-parse-resolve-info": "^4.0.0",
    "graphql-playground-middleware-express": "^1.0.0",
    "graphql-tag": "^2.0.0",
    "graphql-type-json": "^0.3.0",
    "io-ts": "^2.0.0",
    "io-ts-types": "^0.5.0",
    "jsonwebtoken": "^8.0.0",
    "monocle-ts": "^2.0.0",
    "msgpack": "^1.0.0",
    "newtype-ts": "^0.3.0",
    "node-fetch": "^2.0.0",
    "ramda": "^0.27.0",
    "redis": "^3.0.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.0.0",
    "@graphql-codegen/typescript": "^1.0.0",
    "@microsoft/api-extractor": "^7.0.0",
    "@pact-foundation/pact": "^9.0.0",
    "@pact-foundation/pact-node": "^10.0.0",
    "@splish-me/changelog": "^0.1.0",
    "@splish-me/copyright-headers": "^0.0.2",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.0.0",
    "@types/msgpack": "^0.0.30",
    "@types/node": "^12.0.0",
    "@types/node-fetch": "^2.0.0",
    "@types/ramda": "^0.27.0",
    "@types/redis": "^2.0.0",
    "@types/rimraf": "^3.0.0",
    "@types/semver": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "apollo-server-testing": "^2.0.0",
    "babel-plugin-import-graphql": "^2.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-react": "^7.0.0",
    "glob": "^7.0.0",
    "jest": "^26.0.0",
    "jest-transform-graphql": "^2.0.0",
    "msw": "^0.20.0",
    "npm-run-all": "^4.0.0",
    "prettier": "^2.0.0",
    "prettier-plugin-packagejson": "^2.0.0",
    "rimraf": "^3.0.0",
    "semver": "^7.0.0",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.0.0",
    "tsdx": "^0.13.0",
    "typescript": "^3.9.0"
  },
  "engines": {
    "node": "^12.0.0"
  }
}
