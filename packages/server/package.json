  "main": "dist/index.js",
  "module": "dist/authorization.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "_build": "tsdx build --tsconfig tsconfig.prod.json",
    "build:server": "yarn _build --format cjs --target node --entry src/server.ts",
    "build:swr-queue-worker": "yarn _build --format cjs --target node --entry src/swr-queue-worker.ts",
    "codegen": "graphql-codegen --config codegen.yml",
    "deploy:image": "ts-node scripts/deploy",
    "prestart": "lerna run build",
    "start": "tsdx watch --format cjs --target node --entry src/server.ts --tsconfig tsconfig.prod.json --onSuccess \"node dist\"",
    "start:swr-queue-worker": "tsdx watch --format cjs --target node --entry src/swr-queue-worker.ts --tsconfig tsconfig.prod.json --onSuccess \"node dist\""
  },
  "dependencies": {
    "@nmshd/connector-sdk": "^1.1.5",
