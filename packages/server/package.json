    "io-ts": "^2.0.0",
    "io-ts-reporters": "^1.0.0",
    "io-ts-types": "^0.5.0",
    "jsonwebtoken": "^8.0.0",
    "loglevel": "^1.0.0",
    "monocle-ts": "^2.0.0",
    "msgpack5": "^5.0.0",
    "newtype-ts": "^0.3.0",
    "node-fetch": "^2.6.1",
    "ramda": "^0.27.0",
    "redis": "^3.0.0",
    "redlock": "^3.0.0",
    "ts-toolbelt": "^9.0.0"
  },
