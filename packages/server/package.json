    "@types/ramda": "^0.27.37",
    "@types/redis": "^2.0.0",
    "@types/redlock": "^3.0.0",
    "@types/semver": "^7.0.0",
    "@zerollup/ts-transform-paths": "^1.0.0",
    "apollo-server-testing": "^2.0.0",
    "babel-plugin-import-graphql": "^2.0.0",
