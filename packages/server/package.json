    "apollo-server": "^2.0.0",
    "apollo-server-caching": "^0.7.0",
    "apollo-server-express": "^2.0.0",
    "apollo-server-types": "^0.8.0",
    "basic-auth": "^2.0.0",
    "bee-queue": "^1.0.0",
    "body-parser": "^1.19.0",
    "bull-arena": "^3.0.0",
    "dotenv": "^9.0.0",
    "express": "^4.0.0",
    "fp-ts": "^2.0.0",
