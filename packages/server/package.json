{
  "name": "@serlo/api.serlo.org",
  "version": "0.53.3",
  "private": true,
  "repository": "serlo/api.serlo.org",
  "license": "Apache-2.0",
  "author": "Serlo Education e.V.",
  "main": "dist/index.js",
  "module": "dist/authorization.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "_build": "tsdx build --transpileOnly --tsconfig tsconfig.prod.json",
    "build:server": "yarn _build --format cjs --target node --entry src/server.ts",
    "build:swr-queue-worker": "yarn _build --format cjs --target node --entry src/swr-queue-worker.ts",
    "codegen": "graphql-codegen --config codegen.yml",
    "deploy:image": "NODE_OPTIONS='--experimental-specifier-resolution=node' ts-node-esm scripts/deploy.ts",
    "prestart": "lerna run build",
    "start": "tsdx watch --transpileOnly --format cjs --target node --entry src/server.ts --tsconfig tsconfig.prod.json --onSuccess \"node dist\"",
    "start:swr-queue-worker": "tsdx watch --transpileOnly --format cjs --target node --entry src/swr-queue-worker.ts --tsconfig tsconfig.prod.json --onSuccess \"node dist\""
  },
  "dependencies": {
    "@apollo/server": "^4.9.0",
    "@google-cloud/storage": "^7.0.0",
    "@ory/client": "^1.1.41",
    "@sentry/node": "^7.61.0",
    "@serlo/authorization": "^0.53.3",
    "apollo-datasource-rest": "^3.7.0",
    "basic-auth": "^2.0.1",
    "bee-queue": "^1.5.0",
    "bull-arena": "^3.30.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fp-ts": "^2.16.1",
    "graphql": "^16.7.1",
    "graphql-playground-middleware-express": "^1.7.23",
    "graphql-type-json": "^0.3.2",
    "io-ts": "^2.2.20",
    "io-ts-reporters": "^2.0.1",
    "io-ts-types": "^0.5.19",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.1",
    "loglevel": "^1.8.1",
    "monocle-ts": "^2.3.13",
    "msgpack5": "^6.0.2",
    "newtype-ts": "^0.3.5",
    "pg": "^8.11.1",
    "ramda": "^0.29.0",
    "redlock": "^5.0.0-beta.2",
    "ts-toolbelt": "^9.6.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/basic-auth": "^1.1.3",
    "@types/bull-arena": "^3.0.7",
    "@types/graphql-modules": "^0.53.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/pg": "^8.10.2",
    "@types/ramda": "^0.29.3",
    "@types/redlock": "^4.0.4",
    "@types/semver": "^7.5.0",
    "@types/uuid": "^9.0.2",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-plugin-import-graphql": "^2.8.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "semver": "^7.5.4"
  }
}
